# OpenAI Configuration
OPENAI_API_KEY=OPENAI_API_KEY


# Model Configuration
ASSISTANT_MODEL=gpt-3.5-turbo
MODERATOR_MODEL=gpt-4

# Server Configuration
SERVER_PORT=3000
LOG_LEVEL=info

# LLM Parameters
MAX_TOKENS=500
TEMPERATURE=0.7

# Rate Limiting
RATE_LIMIT_PER_MIN=60

# Timeouts
REQUEST_TIMEOUT=30s

# Features
ENABLE_METRICS=true
CACHE_ENABLED=true
CACHE_TTL=1h